{
  "name": "Google Cloud Console Production Warning",
  "description": "Show a warning when viewing a production environment in the Google Cloud Console.",
  "version": "1.0.3",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icon16x16.png",
    "48": "icon48x48.png",
    "128": "icon128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://console.cloud.google.com/*",
        "https://*.appspot.com/_ah/datastore_admin*"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "manifest_version": 2
}
